{
  "name": "carno-php/log",
  "license": "MIT",
  "provide": {
    "psr/log-implementation": "1.0"
  },
  "require": {
    "carno-php/container": "^1.0",
    "carno-php/promise": "^1.0",
    "carno-php/config": "~2.0",
    "carno-php/timer": "^1.0",
    "carno-php/net": "^1.0",
    "carno-php/env": "^1.0",
    "psr/log": "~1.0"
  },
  "suggest": {
    "carno-php/socket": "Supports output to TCP/UDP"
  },
  "replace": {
    "carno-php/log-slim": "^1.0|~2.0"
  },
  "autoload": {
    "psr-4": {
      "Carno\\Log\\": "src"
    },
    "files": ["utils.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Carno\\Log\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
